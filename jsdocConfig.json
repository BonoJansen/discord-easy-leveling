{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|src/xpChart.js|src/deletedb.js|src/events)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "encoding": "utf8",
    "verbose": true,
    "recurse": true,
    "destination": "./docs/",
    "tutorials": "./tutorials",
    "readme": "./readme.md",
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "dark",
      "meta": [
        {
          "name": "author",
          "content": "retrouser955"
        },
        {
          "name": "description",
          "content": "A complete framework to implement a leveling system using discord.js v13"
        }
      ],
      "title": "Discord Easy Leveling"
    }
  }
}